{
  "name": "storybook-react-template",
  "version": "0.0.0",
  "description": "",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build": "npm run clean && npm run build-js",
    "build-js": "cross-env NODE_ENV=production babel src --ignore src/stories --extensions \".js,.jsx\" --out-dir dist",
    "build-storybook": "build-storybook",
    "lint": "eslint --fix src",
    "clean": "rimraf dist",
    "prepare": "husky install",
    "release": "standard-version"
  },
  "lint-staged": {
    "*.{js,jsx,css,json,md}": "prettier --write",
    "*.{js,jsx}": "eslint --fix"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "dependencies": {},
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@storybook/addon-essentials": "^6.4.17",
    "@storybook/react": "^6.4.17",
    "@storybook/theming": "^6.4.17",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.6.0",
    "eslint-config-react-app": "^7.0.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "prettier": "^2.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^4.0.4",
    "standard-version": "^9.3.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.7.0"
  },
  "license": "MIT"
}
